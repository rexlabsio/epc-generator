{
    "name": "rexlabs/epc-generator",
    "description": "Energy Performance Graph Generator",
    "keywords": [
        "epc",
        "energy performance certificate",
        "epc graph generator"
    ],
    "license": "MIT",
    "homepage": "https://github.com/rexlabsio/epc-generator",
    "authors": [
        {
            "name": "Andrea Marco Sartori",
            "email": "andrea.sartori@rexsoftware.com.au",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.4 <8.3",
        "ext-gd": "*",
        "intervention/image": "^2.4"
    },
    "require-dev": {
        "mikey179/vfsstream": "^1.6",
        "phpunit/phpunit": "^9"
    },
    "autoload": {
        "psr-4": {
            "Rexlabs\\EpcGenerator\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rexlabs\\EpcGenerator\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    }
}
